#include <iostream>
using namespace std;
// Esta base de datos trabajara de acuerdo a los personajes del juego League of Legends y a las estadisticas de estos que cambian con objetos durante las partidas
class Champ{ // Clase definida para los personajes
  private:
    string name;
    string range;
    int AD;
    int AP;
    int AS;
    int HP;
    int HR;
    int MR;
    int AR;
    int MS;
  public:
    Champ(string);
    void leer();
};

Champ::Champ (string _name){
  name = _name;

}

class ObjPas{ // Clase definida para los objetos solo pasivos enteros
  private:
    string name;
    int ADo;
    int APo;
    int ASo;
    int HPo;
    int HRo;
    int MRo;
    int ARo;
    int MSo;
  public:
    ObjPas(string);
    void leer();
    int add();
};

class ObjPer{ // Clase definida para los objetos solo pasivos con porcentajes
  private:
    string name;
    float ADo;
    float APo;
    float ASo;
    float HPo;
    float HRo;
    float MRo;
    float ARo;
    float MSo;
  public:
    ObjPas(string);
    void leer();
    int add();
};


class ObjAct{ // Clase definida para los objetos activables
  private:
    string name;
    int ADo;
    int APo;
    int ASo;
    int HPo;
    int HRo;
    int MRo;
    int ARo;
    int MSo;
    int CD;
    int DT;
  public:
    ObjPas(string);
    void leer();
    int add();
};

class ObjChar { // Clase definida para los objetos con cargas
  private:
    string name;
    int ADo;
    int APo;
    int ASo;
    int HPo;
    int HRo;
    int MRo;
    int ARo;
    int MSo;
    int CN;
  public:
    ObjPas(string);
    void leer();
    int add();
};


int main() {
  std::cout << "Hello World!\n";
}
